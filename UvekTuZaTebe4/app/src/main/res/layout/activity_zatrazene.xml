<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/custom_button"
    tools:context=".korisnik.Zatrazene">


    <ImageButton
        android:id="@+id/logaut2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/ic_baseline_logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.97"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.02"
        android:contentDescription="@string/opis1"/>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/risajklzatrazene3"
        android:layout_width="match_parent"
        android:layout_height="388dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.31" />

    <ImageButton
        android:id="@+id/nazadbatn"
        android:layout_width="44dp"
        android:layout_height="48dp"
        android:background="#00000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.02"
        app:srcCompat="@drawable/ic_baseline_west_24"
        android:contentDescription="@string/opis1"/>

    <TextView
        android:id="@+id/zdravotekst"
        android:layout_width="271dp"
        android:layout_height="80dp"
        android:fontFamily="@font/carter_one"
        android:text="@string/zdravo_pantelicuros02"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.024" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="39dp"
        android:fontFamily="@font/carter_one"
        android:text="@string/nas_sajt"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/risajklzatrazene3"
        app:layout_constraintVertical_bias="0.217" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="39dp"
        android:fontFamily="@font/carter_one"
        android:text="@string/nas_facebook"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.065"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/risajklzatrazene3"
        app:layout_constraintVertical_bias="0.51" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:fontFamily="@font/carter_one"
        android:text="@string/nas_instagram"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/risajklzatrazene3"
        app:layout_constraintVertical_bias="0.787" />

    <ImageButton
        android:id="@+id/sajt1"
        android:layout_width="100dp"
        android:layout_height="39dp"
        android:layout_marginBottom="176dp"
        android:background="@color/white"
        android:contentDescription="@string/opis1"
        app:layout_constraintBottom_toBottomOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/fejs1"
        android:layout_width="100dp"
        android:layout_height="39dp"
        android:layout_marginBottom="124dp"
        android:background="@color/purple_500"
        android:contentDescription="@string/opis1"
        app:layout_constraintBottom_toBottomOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.11"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/insta1"
        android:layout_width="100dp"
        android:layout_height="39dp"
        android:background="#FF4FE7"
        android:contentDescription="@string/opis1"
        app:layout_constraintBottom_toBottomOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.649"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView6"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView71"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/carter_one"
        android:text="@string/kliknite_ovde"
        android:textColor="#DD2C00"
        app:layout_constraintBottom_toBottomOf="@+id/sajt1"
        app:layout_constraintEnd_toEndOf="@+id/sajt1"
        app:layout_constraintStart_toStartOf="@+id/sajt1"
        app:layout_constraintTop_toTopOf="@+id/sajt1" />

    <TextView
        android:id="@+id/textView81"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/fredoka_one"
        android:text="@string/kliknite_ovde"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/fejs1"
        app:layout_constraintEnd_toEndOf="@+id/fejs1"
        app:layout_constraintStart_toStartOf="@+id/fejs1"
        app:layout_constraintTop_toTopOf="@+id/fejs1" />

    <TextView
        android:id="@+id/textView91"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/carter_one"
        android:text="@string/kliknite_ovde"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/insta1"
        app:layout_constraintEnd_toEndOf="@+id/insta1"
        app:layout_constraintStart_toStartOf="@+id/insta1"
        app:layout_constraintTop_toTopOf="@+id/insta1" />


</androidx.constraintlayout.widget.ConstraintLayout>